    <PolicySet description="Vancouver FDU-Secure WLAN Policy" name="V_FDU_Secure">
      <Conditions relationship="AND">
        <Condition name="Wireless_802.1X" type="REUSABLE_COMPOUND"/>
        <Condition name="Vancouver" type="REUSABLE_COMPOUND"/>
      </Conditions>
      <Authentication>
        <rules>
          <rule name="Guest_RADIUS_PROXY" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="Guest_Username" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <Result name="FDU_RADIUS_SEQ_VANC" type="RADIUSServerSequence"/>
            <IdentitySourceRules/>
          </rule>
          <rule name="External_RADIUS_PROXY" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="External_Realm_Username" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <Result name="FDU_RADIUS_SEQ_VANC" type="RADIUSServerSequence"/>
            <IdentitySourceRules/>
          </rule>
          <rule name="Student_RADIUS_PROXY" status="Disabled">
            <Conditions relationship="AND">
              <Condition name="Student_Username" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <Result name="FDU_RADIUS_SEQ_VANC" type="RADIUSServerSequence"/>
            <IdentitySourceRules/>
          </rule>
          <rule name="Equipment_RADIUS_PROXY" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="Equipment_Username" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <Result name="FDU_RADIUS_SEQ_VANC" type="RADIUSServerSequence"/>
            <IdentitySourceRules/>
          </rule>
          <rule name="Guest_ISE_RADIUS_PROXY" status="Enabled">
            <Conditions relationship="OR">
              <Condition type="ADHOC">Radius:User-Name STARTS WITH fdu-guest-</Condition>
              <Condition type="ADHOC">Radius:User-Name STARTS WITH ROOT\fdu-guest-</Condition>
            </Conditions>
            <Result name="FDU_RADIUS_ISE" type="RADIUSServerSequence"/>
            <IdentitySourceRules/>
          </rule>
          <rule name="Default" status="Enabled">
            <Conditions/>
            <Result name="Default Network Access" type="AllowedProtocolServices"/>
            <IdentitySourceRules>
              <rule name="Default" status="Enabled">
                <Conditions/>
                <IdentitySourceResult name="FDU_AD_Internal">
                  <IdentitySource name="FDU_AD_Internal" type="IdentityStoreSequence"/>
                  <AuthenFailed>REJECT</AuthenFailed>
                  <UserNotFound>REJECT</UserNotFound>
                  <ProcessFailed>DROP</ProcessFailed>
                </IdentitySourceResult>
              </rule>
            </IdentitySourceRules>
