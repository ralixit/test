  <!--This section describes the Reusable Conditions configured in ISE-->
  <ReusableConditions>
    <Authentication>
      <Compound>
        <condition description="A condition to match MAC Authentication Bypass service requests from Cisco Catalyst Switches" name="Wired_MAB" relationship="AND">
          <Condition type="ADHOC">Radius:Service-Type EQUALS Call Check</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Ethernet</Condition>
        </condition>
        <condition description="A condition to match MAC Authentication Bypass service requests from Cisco Wireless LAN Controller" name="Wireless_MAB" relationship="AND">
          <Condition type="ADHOC">Radius:Service-Type EQUALS Call Check</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Wireless - IEEE 802.11</Condition>
        </condition>
        <condition description="A condition to match an 802.1X based authentication requests from Cisco Catalyst Switches" name="Wired_802.1X" relationship="AND">
          <Condition type="ADHOC">Radius:Service-Type EQUALS Framed</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Ethernet</Condition>
        </condition>
        <condition description="A condition to match an 802.1X based authentication request from Cisco Wireless LAN Controller" name="Wireless_802.1X" relationship="AND">
          <Condition type="ADHOC">Radius:Service-Type EQUALS Framed</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Wireless - IEEE 802.11</Condition>
        </condition>
        <condition description="A condition to match authentication requests for Local Web Authentication from Cisco Catalyst Switches" name="Switch_Local_Web_Authentication" relationship="AND">
          <Condition type="ADHOC">Radius:Service-Type EQUALS Outbound</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Ethernet</Condition>
        </condition>
        <condition description="A condition to match authenticaiton requests for Web Authentication from Cisco Wireless LAN  Controller" name="WLC_Web_Authentication" relationship="AND">
          <Condition type="ADHOC">Radius:Service-Type EQUALS Login</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Wireless - IEEE 802.11</Condition>
        </condition>
        <condition description="A condition to match MAC Authentication Bypass service requests from Cisco Wireless LAN Controller" name="Wireless_MAB_copy" relationship="AND">
          <Condition type="ADHOC">Radius:Service-Type EQUALS Call Check</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Wireless - IEEE 802.11</Condition>
        </condition>
        <condition description="" name="AD_Usernames" relationship="OR">
          <Condition type="ADHOC">Radius:User-Name STARTS WITH root\</Condition>
          <Condition type="ADHOC">Radius:User-Name STARTS WITH host/</Condition>
        </condition>
        <condition description="" name="Madison" relationship="OR">
          <Condition type="ADHOC">DEVICE:Location EQUALS Location#All Locations#Madison</Condition>
          <Condition type="ADHOC">DEVICE:Location EQUALS Location#All Locations#Madison_RH</Condition>
        </condition>
        <condition description="" name="Teaneck" relationship="OR">
          <Condition type="ADHOC">DEVICE:Location EQUALS Location#All Locations#Teaneck</Condition>
          <Condition type="ADHOC">DEVICE:Location EQUALS Location#All Locations#Teaneck_RH</Condition>
        </condition>
        <condition description="Username contains @student.fdu.edu" name="Student_Username" relationship="AND">
          <Condition type="ADHOC">Radius:User-Name MATCHES .*(@student.fdu.edu)$</Condition>
        </condition>
        <condition description="Username contains @equipment.fdu.edu" name="Equipment_Username" relationship="AND">
          <Condition type="ADHOC">Radius:User-Name MATCHES .*(@equipment.fdu.edu)$</Condition>
        </condition>
        <condition description="Username contains @group.fdu.edu" name="Group_Username" relationship="AND">
          <Condition type="ADHOC">Radius:User-Name MATCHES .*(@group.fdu.edu)$</Condition>
        </condition>
        <condition description="Username contains @esl.fdu.edu" name="ESL_Username" relationship="AND">
          <Condition type="ADHOC">Radius:User-Name MATCHES .*(@esl.fdu.edu)$</Condition>
        </condition>
        <condition description="Username contains @gsa.fdu.edu" name="GSA_Username" relationship="AND">
          <Condition type="ADHOC">Radius:User-Name MATCHES .*(@gsa.fdu.edu)$</Condition>
        </condition>
        <condition description="Usernames that are from an External Realm like gsa.fdu.edu, esl, or group" name="External_Realm_Username" relationship="OR">
          <Condition type="ADHOC">Radius:User-Name MATCHES .*(@gsa.fdu.edu)$</Condition>
          <Condition type="ADHOC">Radius:User-Name MATCHES .*(@esl.fdu.edu)$</Condition>
          <Condition type="ADHOC">Radius:User-Name MATCHES .*(@group.fdu.edu)$</Condition>
        </condition>
        <condition description="Device Type is Meraki AP" name="Meraki_AP" relationship="AND">
          <Condition type="ADHOC">DEVICE:Device Type EQUALS Device Type#All Device Types#Meraki AP</Condition>
        </condition>
        <condition description="FDU Users visiting other Institutions" name="eduRoam_Traveling_FDU_Users" relationship="AND">
          <Condition type="ADHOC">Radius:Service-Type EQUALS Framed</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Wireless - IEEE 802.11</Condition>
          <Condition type="ADHOC">Airespace:Airespace-Wlan-Id EQUALS 96</Condition>
        </condition>
        <condition description="Visiting eduRoam External Users" name="eduRoam_External_User" relationship="AND">
          <Condition type="ADHOC">Radius:User-Name NOT ENDS WITH fdu.edu</Condition>
          <Condition type="ADHOC">Radius:User-Name CONTAINS @</Condition>
          <Condition type="ADHOC">Radius:Service-Type EQUALS Framed</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Wireless - IEEE 802.11</Condition>
        </condition>
        <condition description="Traffic coming from the Wireless-LAN-ID  6 (eduRoam)" name="eduRoam_SSID" relationship="AND">
          <Condition type="ADHOC">Radius:Service-Type EQUALS Framed</Condition>
          <Condition type="ADHOC">Radius:NAS-Port-Type EQUALS Wireless - IEEE 802.11</Condition>
          <Condition type="ADHOC">Airespace:Airespace-Wlan-Id EQUALS 6</Condition>
        </condition>
        <condition description="" name="Vancouver" relationship="OR">
          <Condition type="ADHOC">DEVICE:Location EQUALS Location#All Locations#Vancouver</Condition>
        </condition>
        <condition description="Username contains @guest.fdu.edu" name="Guest_Username" relationship="OR">
          <Condition type="ADHOC">Radius:User-Name MATCHES .*(@guest.fdu.edu)$</Condition>
          <Condition type="ADHOC">Radius:User-Name CONTAINS @guest.fdu.edu</Condition>
        </condition>
      </Compound>
    </Authentication>
    <Authorization>
      <Compound>
