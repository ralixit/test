<?xml version="1.0" encoding="UTF-8"?><Root>
  <!--This section describes the Policy-Sets configured in ISE-->
  <PolicySets>
    <PolicySet description="" name="VPN">
      <Conditions relationship="AND">
        <Condition type="ADHOC">DEVICE:Device Type EQUALS Device Type#All Device Types#VPN</Condition>
      </Conditions>
      <Authentication>
        <rules>
          <rule name="Default" status="Enabled">
            <Conditions/>
            <Result name="Default Network Access" type="AllowedProtocolServices"/>
            <IdentitySourceRules>
              <rule name="Default" status="Enabled">
                <Conditions/>
                <IdentitySourceResult name="FDU_AD_Internal_RADIUS_VPN">
                  <IdentitySource name="FDU_AD_Internal_RADIUS_VPN" type="IdentityStoreSequence"/>
                  <AuthenFailed>REJECT</AuthenFailed>
                  <UserNotFound>REJECT</UserNotFound>
                  <ProcessFailed>DROP</ProcessFailed>
                </IdentitySourceResult>
              </rule>
            </IdentitySourceRules>
          </rule>
        </rules>
      </Authentication>
      <Authorization>
        <StandardRules>
          <rule name="Employee VPN User" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="Employee" type="REUSABLE_COMPOUND"/>
              <Condition name="Employee_VPN_Tunnel" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <identityGroups>
              <identityGroup name="Any"/>
            </identityGroups>
            <Result name="PermitAccess" type="Standard"/>
          </rule>
          <rule name="Student VPN User" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="Student" type="REUSABLE_COMPOUND"/>
              <Condition name="Student_VPN_Tunnel" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <identityGroups>
              <identityGroup name="Any"/>
            </identityGroups>
            <Result name="PermitAccess" type="Standard"/>
          </rule>
          <rule name="Alumni VPN User" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="Alumni_VPN_Tunnel" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <identityGroups>
              <identityGroup name="Any"/>
            </identityGroups>
            <Result name="PermitAccess" type="Standard"/>
          </rule>
          <rule name="Affiliate VPN User" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="Community_VPN_Tunnel" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <identityGroups>
              <identityGroup name="Any"/>
            </identityGroups>
            <Result name="PermitAccess" type="Standard"/>
          </rule>
          <rule name="Admin VPN User" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="Admin_VPN_Tunnel" type="REUSABLE_COMPOUND"/>
              <Condition name="Employee" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <identityGroups>
              <identityGroup name="Any"/>
            </identityGroups>
            <Result name="PermitAccess" type="Standard"/>
          </rule>
          <rule name="Default" status="Enabled">
            <Conditions/>
            <identityGroups>
              <identityGroup name="Any"/>
            </identityGroups>
            <Result name="DenyAccess" type="Standard"/>
          </rule>
        </StandardRules>
        <LocalExceptionRules/>
      </Authorization>
    </PolicySet>
    <PolicySet description="Allow Inbound FDU Authentications from EduRoam" name="eduRoam_Inbound">
      <Conditions relationship="AND">
        <Condition type="ADHOC">DEVICE:Location EQUALS Location#All Locations#External eduRoam</Condition>
        <Condition type="ADHOC">DEVICE:Device Type EQUALS Device Type#All Device Types#eduRoam</Condition>
      </Conditions>
      <Authentication>
        <rules>
          <rule name="Default" status="Enabled">
            <Conditions/>
            <Result name="Default Network Access" type="AllowedProtocolServices"/>
