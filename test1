        </StandardRules>
        <LocalExceptionRules/>
      </Authorization>
    </PolicySet>
    <PolicySet description="Teaneck FDU-Secure WLAN Policy" name="T_eduRoam">
      <Conditions relationship="AND">
        <Condition name="Wireless_802.1X" type="REUSABLE_COMPOUND"/>
        <Condition name="Teaneck" type="REUSABLE_COMPOUND"/>
        <Condition type="ADHOC">Airespace:Airespace-Wlan-Id EQUALS 6</Condition>
      </Conditions>
      <Authentication>
        <rules>
          <rule name="eduRoam_Users" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="eduRoam_External_User" type="REUSABLE_COMPOUND"/>
            </Conditions>
            <Result name="eduRoam" type="RADIUSServerSequence"/>
            <IdentitySourceRules/>
          </rule>
          <rule name="Guest_RADIUS_PROXY" status="Enabled">
