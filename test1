            </identityGroups>
            <Result name="Blackhole_Wireless_Access" type="Standard"/>
          </rule>
          <rule name="Domain Computers" status="Enabled">
            <Conditions relationship="AND">
              <Condition name="AD_Username" type="REUSABLE_COMPOUND"/>
              <Condition type="ADHOC">root:ExternalGroups EQUALS root.fdu.edu/Users/Domain Computers</Condition>
            </Conditions>
            <identityGroups>
              <identityGroup name="Any"/>
            </identityGroups>
            <Result name="T_Employee_Wireless" type="Standard"/>
          </rule>
          <rule name="Employee Domain Users" status="Enabled">
